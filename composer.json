{
    "name": "keithbrink/affiliates-spark",
    "description": "An affiliates package for Laravel Spark that allows you to track referrals from affiliates and pay commission",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KeithBrink\\AffiliatesSpark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AffiliatesSpark\\Tests\\": "tests"
        }
    },
    "require": {
        "laravel/cashier": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4.0",
        "orchestra/testbench": "^3.5",
        "illuminate/database": "~5",
        "mockery/mockery": "^1.2"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "KeithBrink\\AffiliatesSpark\\AffiliatesSparkServiceProvider"
            ]
        }
    }
}
